{
  "name": "Display all visible",
  "description": "All non-visible elements to display the.",
  "version": "1.0.0",
  "background": {
    "scripts": ["bk.js"]
  },
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],
  "browser_action": {
      "default_title": "Display all visible",
      "default_icon": "icon_01.png"
      // "default_popup" : "popup.html"
  },
  "content_scripts": [
      {
        "matches": ["http://*/*", "https://*/*"],
        "css": ["style.css"]
      }
  ],
  "manifest_version": 2
}